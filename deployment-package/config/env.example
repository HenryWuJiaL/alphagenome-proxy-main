# AlphaGenome API 配置
# 从 https://console.cloud.google.com/apis/credentials 获取
ALPHAGENOME_API_KEY=your_alphagenome_api_key_here

# Google Cloud 配置
PROJECT_ID=your_project_id_here
REGION=us-central1
SERVICE_NAME=alphagenome-proxy

# 服务配置
PORT=8080
MEMORY=512Mi
CPU=1
MAX_INSTANCES=10

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=json

# 安全配置
CORS_ORIGINS=["*"]
ALLOWED_HOSTS=["*"]

# 性能配置
WORKERS=1
TIMEOUT=60 